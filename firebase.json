{
  "hosting": {
    "public": "public",
    "rewrites": [
      {
      "source": "**",
      "destination": "/index.html"
      }
    ],
    "headers": [
      {
        "source" : "**/*.@(jpg|jpeg|gif|png|ico)",
        "headers" : [ {
          "key" : "Cache-Control",
          "value" : "max-age=7200"
        }]
      },
      {
        "source" : "sw.js",
        "headers" : [
          {
            "key" : "Cache-Control",
            "value" : "max-age=0, no-cache, no-store, must-revalidate"
          }
        ]
      },
      {
        "source": "/",
        "headers": [{"key": "link", "value": "</bundle.js>;rel=preload;as=script,</sw.js>;rel=preload;as=script"}]
      }
    ]
  }
}
